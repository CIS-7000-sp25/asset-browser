{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".", // required for "paths" to work
    "paths": {
      "@/*": ["src/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "esnext"
  }
}