{
    "build": {
      "builder": "NIXPACKS",
      "nixpacksPlan": {
        "phases": {
          "setup": {
            "commands": ["pip install -r backend/requirements.txt"]
          },
          "start": {
            "cmd": "cd backend && python manage.py migrate && gunicorn asset_library.wsgi:application"
          }
        }
      }
    }
  }
  